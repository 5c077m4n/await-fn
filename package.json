{
	"name": "await-fn",
	"version": "1.2.51",
	"description": "A tiny package to await a classic function (and more...).",
	"main": "./dist/to.cjs.bundle.js",
	"module": "./dist/to.esm.bundle.js",
	"scripts": {
		"clean": "rimraf ./dist",
		"test": "mocha",
		"lint": "eslint ./src/**/*.js",
		"lint:staged": "lint-staged",
		"build": "webpack --config webpack.config.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/5c077m4n/await-fn.git"
	},
	"keywords": [
		"await",
		"function",
		"async",
		"backend"
	],
	"author": "Roee Shapira",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/5c077m4n/await-fn/issues"
	},
	"homepage": "https://5c077m4n.github.io/await-fn/",
	"dependencies": {
		"http-responder": "^2.0.20"
	},
	"devDependencies": {
		"@babel/core": "^7.7.4",
		"@babel/plugin-proposal-object-rest-spread": "^7.7.4",
		"@babel/preset-env": "^7.7.4",
		"babel-loader": "^8.0.6",
		"chai": "^4.2.0",
		"eslint": "^6.7.2",
		"husky": "^3.1.0",
		"lint-staged": "^9.5.0",
		"mocha": "^6.2.2",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"husky": {
		"hooks": {
			"pre-commit": "run-s lint:staged clean build",
			"pre-push": "run-s lint test"
		}
	},
	"lint-staged": {
		"*.js": [
			"eslint --fix --ignore-pattern 'dist/**'",
			"git add"
		]
	}
